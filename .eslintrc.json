{
  "extends": "next/core-web-vitals",
  "overrides": [
    {
      "files": ["**/*.ts?(x)"],
      "plugins": [
        "@typescript-eslint"
      ],
      "extends": [
        "plugin:@typescript-eslint/recommended"
      ],
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          "error",
          {
            "argsIgnorePattern": "^_.*"
          }
        ]
      }
    }
  ]
}
